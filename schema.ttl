@base <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix focu: <http://focu.io/schema#> .
@prefix dbo: <http://www.dbpedia.org/ontology/>.
@prefix dbr: <http://www.dbpedia.org/resource/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .

<Concordia>
	dbo:type dbr:Public_university;
	foaf:name "Concordia University";
	rdfs:seeAlso dbr:Concordia_University.

focu:Student
	a rdfs:Class;
	rdfs:subClassOf foaf:Person;
	rdfs:label "Student"@en;
	rdfs:comment "A person enrolled in one or more courses at a academic institution or other place of academic learning"@en.

focu:Lecture
	a rdfs:Class;
	rdfs:subClassOf dcmitype:Event;
	rdfs:label "Lecture"@en;
	rdfs:comment "Information about lectures in a course"@en.

focu:Course
	a rdfs:Class;
	rdfs:subClassOf  dbr:Course_\(education\);
	rdfs:label "Unique identifier for a course"@en;
	rdfs:comment "An abbreviated course displicine and course number which combined uniquely identifies any university course within that academic institution."@en.

focu:courseTaken
	a rdf:property ;
	rdfs:domain <Student> ;
	rdfs:range <Course>;
    rdfs:label "Courses Taken"@en ;
    rdfs:comment "Courses enrolled into by a student"@en .

focu:hasContent
    a rdf:property ;
    rdfs:subClassOf dcterms:relation ;
	rdfs:domain <Lecture> ;
	rdfs:range xsd:string;
    rdfs:label "Content"@en ;
    rdfs:comment "Content associated with a course, lecture"@en .

focu:competencies
	a rdf:Property;
	rdfs:domain <Student>;
	rdfs:range xsd:string;
	rdfs:label "Competencies"@en;
	rdfs:comment "Competencies, defined as a set of topics, based on the courses a student successfully passed"@en.

focu:Record
	a dbo:document;
	rdfs:label "Record"@en;
	rdfs:comment "Information about single student's participation in a single course"@en .

focu:hasRecord
	a rdf:Property;
	rdfs:domain <Student>;
	rdfs:range <Record>;
	rdfs:label "Has record"@en;
	rdfs:comment "A property describing the relationship between a single student and a single record"@en.

focu:subject
	a rdf:Property;
	rdfs:domain <course>;
	rdfs:range xsd:string;
	rdfs:label "Discipline of course"@en;
	rdfs:comment "An abbreviation for the discipline or department under which an academic course falls"@en.

focu:catalog
	a rdf:Property;
	rdfs:domain <course>;
	rdfs:range xsd:string;
	rdfs:label "Course number"@en;
	rdfs:comment "An integer number which uniquely identifies a course within a particular discipline."@en.

focu:credits
	a rdf:Property;
	rdfs:domain <course>;
	rdfs:range xsd:int;
	rdfs:label "Letter grade"@en;
	rdfs:comment "A single letter measuring the performance in the course of the associated Record."@en.

focu:grade
	a rdf:Property;
	rdfs:domain <Record>;
	rdfs:range xsd:string;
	rdfs:label "Letter grade"@en;
	rdfs:comment "A single letter measuring the performance in the course of the associated Record."@en.

focu:provenance
	a rdf:Property;
	rdfs:domain foaf:topic;
	rdfs:range xsd:string;
	rdfs:label "Provenance"@en;
	rdfs:comment "Record of where the topic was identified as being covered in the course"@en.

focu:firstName
	a rdf:Property;
	rdfs:domain <Student>;
	rdfs:range xsd:string;
	rdfs:label "First Name"@en;
	rdfs:comment "First Name of a student"@en.

focu:lastName
	a rdf:Property;
	rdfs:domain <Student>;
	rdfs:range xsd:string;
	rdfs:label "Last Name"@en;
	rdfs:comment "Last Name of a student"@en.

focu:offeredAt
	a rdf:Property;
	rdfs:domain focu:Course;
	rdfs:range dbr:Public_university;
	rdfs:label "Offered At"@en;
	rdfs:comment "A university course is offered at the associated university."@en.

